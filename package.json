{
  "name": "hades-base-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start tool-translate-language --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/hades-base-nestjs/test/jest-e2e.json",
    "start:jwt": "nest start --watch jwt-authentication",
    "start:tool": "nest start --watch tool-translate-language",
    "build:tool": "nest build tool-translate-language",
    "build:db": "nest build database-type-orm",
    "migration:run": "npx typeorm-ts-node-esm migration:run -d ./libs/database-type-orm/src/data-source.ts",
    "migration:create": "npx typeorm-ts-node-esm migration:create ./libs/database-type-orm/src/migrations",
    "migration:generate": "npx typeorm-ts-node-esm migration:generate ./libs/database-type-orm/src/migrations -d ./libs/database-type-orm/src/data-source.ts",
    "migration:revert": "npx typeorm-ts-node-esm migration:revert -d ./libs/database-type-orm/src/data-source.ts"
  },
  "dependencies": {
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/throttler": "^3.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@sendgrid/mail": "^7.7.0",
    "ajv": "^8.1.0",
    "ajv-formats": "^2.0.2",
    "aws-sdk": "^2.1260.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.2",
    "cache-manager": "^3.6.0",
    "cache-manager-redis-store": "^2.0.0",
    "dotenv": "^8.6.0",
    "flat": "^5.0.2",
    "ioredis": "^5.2.3",
    "jwks-rsa": "^2.1.5",
    "lodash": "^4.17.21",
    "log4js": "^6.7.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nestjs-throttler-storage-redis": "^0.1.20",
    "onesignal-node": "^3.4.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4",
    "sharp": "^0.31.2",
    "twilio": "^3.83.3",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/cache-manager": "^3.4.3",
    "@types/cache-manager-ioredis": "^2.0.2",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/flat": "^5.0.2",
    "@types/jest": "28.1.4",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.0.0",
    "@types/sharp": "^0.31.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@app/jwt-authentication(|/.*)$": "<rootDir>/libs/jwt-authentication/src/$1",
      "^@app/helpers(|/.*)$": "<rootDir>/libs/helpers/src/$1",
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/database-typeorm(|/.*)$": "<rootDir>/libs/database-typeorm/src/$1",
      "^@app/database-type-orm(|/.*)$": "<rootDir>/libs/database-type-orm/src/$1",
      "^y/authorization(|/.*)$": "<rootDir>/libs/authorization/src/$1",
      "^@app/cache(|/.*)$": "<rootDir>/libs/cache/src/$1",
      "^@app/language(|/.*)$": "<rootDir>/libs/language/src/$1",
      "^@app/s3-upload(|/.*)$": "<rootDir>/libs/s3-upload/src/$1",
      "^@app/resource(|/.*)$": "<rootDir>/libs/resource/src/$1",
      "^@app/send-mail(|/.*)$": "<rootDir>/libs/send-mail/src/$1",
      "^@app/twilio(|/.*)$": "<rootDir>/libs/twilio/src/$1",
      "^@app/queue(|/.*)$": "<rootDir>/libs/queue/src/$1",
      "^@app/notification(|/.*)$": "<rootDir>/libs/notification/src/$1"
    }
  }
}
